{
  "badge-subscription": {
    "prefix": ["bdgsubscription"],
    "body": ["{bdg-primary}`$1`"],
    "description": "Subscription badge."
  },
  "badge-unsupported": {
    "prefix": ["bdgunsupported"],
    "body": ["{bdg-warning}`$1`"],
    "description": "Unsupported/Warning badge."
  },
  "badge-version": {
    "prefix": ["bdgversion"],
    "body": ["{bdg-secondary}`$1`"],
    "description": "Version badge."
  },
  "image": {
    "prefix": ["image"],
    "body": ["![$2]($1)"],
    "description": "Insert image."
  },
  "right-arrow": {
    "prefix": ["ra"],
    "body": ["&rarr;"],
    "description": "Insert right arrow."
  },
  "tokendocker": {
    "prefix": ["tokendocker"],
    "body": ["[$LIFERAY_LEARN_DXP_DOCKER_IMAGE$]"],
    "description": "Insert DXP Docker image token."
  },
  "tokengit": {
    "prefix": ["tokengit"],
    "body": ["[$LIFERAY_LEARN_PORTAL_GIT_TAG$]"],
    "description": "Insert Liferay GitHub token"
  },
  "tokenworkspace": {
    "prefix": ["tokenworkspace"],
    "body": ["[$LIFERAY_LEARN_PORTAL_WORKSPACE_TOKEN$]"],
    "description": "Insert Liferay Workspace token"
  },
  "tokenyoutube": {
    "prefix": ["tokenyoutube"],
    "body": ["[$LIFERAY_LEARN_YOUTUBE_URL$]"],
    "description": "Insert Liferay YouTube token."
  }
}
}
